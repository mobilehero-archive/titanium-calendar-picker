{
	"name": "@titanium/calendar-picker",
	"version": "1.0.0",
	"description": "Turbo widget that allows you to pick dates from a calendar with Axway Titanium.",
	"main": "index.js",
	"scripts": {
		"list": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
		"prep": "npm install --ignore-scripts && npm run list",
		"prepublishOnly": "git pull && bump release",
		"test": "npm run prep",
		"lint": "eslint . --fix"
	},
	"author": "Axway",
	"maintainers": [
		{
			"name": "Brenton House",
			"email": "brenton.house@gmail.com",
			"url": "https://brenton.house"
		}
	],
	"repository": {
		"type": "git",
		"url": "github:brentonhouse/titanium-calendar-picker"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/brentonhouse/titanium-calendar-picker/issues"
	},
	"homepage": "https://github.com/brentonhouse/titanium-calendar-picker#readme",
	"keywords": [
		"titanium",
		"mobile",
		"alloy",
		"turbo",
		"axway",
		"appcelerator",
		"ios",
		"android",
		"iphone",
		"ipad",
		"titanium-widget",
		"brenton-house"
	],
	"titanium": {
		"type": "widget",
		"widgetDir": "./widget",
		"widgetId": "@titanium/calendar-picker",
		"platforms": "ios,android"
	},
	"devDependencies": {
		"@titanium/eslint-config": "1.0.1",
		"eslint": "^7.18.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsdoc": "^36.0.6",
		"eslint-plugin-promise": "^4.2.1",
		"fs-extra": "^9.1.0",
		"lodash": "^4.17.20"
	},
	"dependencies": {}
}
